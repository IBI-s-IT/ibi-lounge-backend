{
  "name": "lounge-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_PATH=. ts-node-dev --transpile-only --require dotenv/config ./src/main.ts | pino-pretty",
    "start": "cross-env NODE_PATH=dist node dist/index.js",
    "build": "tsup-node",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist && rm *.tsbuildinfo",
    "test": "jest --setupFiles dotenv/config",
    "lint": "eslint . --quiet",
    "lint-fix": "eslint . --fix"
  },
  "devDependencies": {
    "eslint": "^9.9.1",
    "@types/jsdom": "^21.1.1",
    "@types/qs": "^6.9.11",
    "cross-env": "^7.0.3",
    "tsup": "8.3.0"
  },
  "dependencies": {
    "@repo/api-schema": "*",
    "@repo/generators": "*",
    "@repo/shared": "*",
    "@fastify/autoload": "~5.7.1",
    "@fastify/cors": "^8.5.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/http-proxy": "^9.5.0",
    "@fastify/redis": "^6.2.0",
    "@fastify/sensible": "~5.2.0",
    "@fastify/swagger": "^8.13.0",
    "@fastify/swagger-ui": "^2.0.1",
    "@fastify/type-provider-json-schema-to-ts": "^2.2.2",
    "ajv": "^8.17.1",
    "axios": "1.6.0",
    "fastify": "^4.25.2",
    "fastify-plugin": "~4.5.0",
    "ical-generator": "^6.0.1",
    "jsdom": "^22.1.0",
    "qs": "^6.11.2",
    "pino": "^8.17.2"
  }
}
