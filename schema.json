{
  "openapi": "3.0.3",
  "info": {
    "title": "IBI Lounge Backend",
    "description": "Бекенд для получения расписания и прочего в удобном формате",
    "version": "2.0.0"
  },
  "components": {
    "schemas": {
      "def-0": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "name": {
            "type": "string"
          }
        },
        "title": "listEntry"
      }
    }
  },
  "paths": {
    "/schedules": {
      "get": {
        "tags": [
          "Расписание"
        ],
        "description": "Выдаёт расписание группы или преподавателя в JSON формате",
        "parameters": [
          {
            "schema": {
              "type": "string"
            },
            "in": "query",
            "name": "dateStart",
            "required": false,
            "description": "Дата формата ДД.ММ.ГГГГ"
          },
          {
            "schema": {
              "type": "string"
            },
            "in": "query",
            "name": "dateEnd",
            "required": false,
            "description": "Дата формата ДД.ММ.ГГГГ"
          },
          {
            "schema": {
              "type": "string"
            },
            "in": "query",
            "name": "group",
            "required": false,
            "description": "ID группы, можно найти через /groups&level=*"
          },
          {
            "schema": {
              "type": "string"
            },
            "in": "query",
            "name": "teacher",
            "required": false,
            "description": "ID преподавателя, можно найти через /teachers"
          },
          {
            "schema": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "subject": {
                    "type": "string"
                  },
                  "group": {
                    "type": "string"
                  },
                  "subgroup": {
                    "type": "string"
                  }
                },
                "required": [
                  "subject",
                  "group",
                  "subgroup"
                ]
              }
            },
            "in": "query",
            "name": "subgroups",
            "required": false,
            "description": "Массив объектов подгрупп: [{ subject: \"string\", group: \"string\", subgroup: \"string\" }]"
          }
        ],
        "responses": {
          "200": {
            "description": "Default Response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "day": {
                        "type": "string"
                      },
                      "month": {
                        "type": "string"
                      },
                      "week_day": {
                        "type": "string"
                      },
                      "lessons": {
                        "type": "array",
                        "items": {
                          "type": "object",
                          "properties": {
                            "time_start": {
                              "type": "string"
                            },
                            "time_end": {
                              "type": "string"
                            },
                            "text": {
                              "type": "string"
                            },
                            "additional": {
                              "type": "object",
                              "required": [
                                "type"
                              ],
                              "properties": {
                                "is_online": {
                                  "type": "boolean"
                                },
                                "type": {
                                  "type": "string",
                                  "enum": [
                                    "unknown",
                                    "practice",
                                    "lecture",
                                    "library_day",
                                    "project_work",
                                    "exam",
                                    "subject_report",
                                    "consultation",
                                    "subject_report_with_grade",
                                    "course_work_defend"
                                  ],
                                  "description": "unknown: Неизвестно; practice: Практика; lecture: Лекция; library_day: Библиотечный день (в 2023 году не встречался); project_work: Проектная деятельность (в 2023 году не встречался); exam: Экзамен; subject_report: Зачёт; consultation: Консультация; subject_report_with_grade: Дифференцированный зачёт; course_work_defend: Защита курсовых работ;",
                                  "additionalProperties": false
                                },
                                "url": {
                                  "type": "string",
                                  "description": "Ссылка"
                                },
                                "group": {
                                  "type": "array",
                                  "items": {
                                    "type": "string"
                                  }
                                },
                                "subgroup": {
                                  "type": "array",
                                  "items": {
                                    "type": "string"
                                  }
                                },
                                "classroom": {
                                  "type": "string",
                                  "description": "Аудитория (простой вариант)"
                                },
                                "teacher_name": {
                                  "type": "string",
                                  "description": "Фамилия и инициалы преподавателя"
                                },
                                "compensation": {
                                  "type": "string",
                                  "description": "Возмещение пары"
                                },
                                "teacher_groups": {
                                  "type": "array",
                                  "items": {
                                    "type": "string"
                                  },
                                  "description": "Группы которые будут на паре (только у учителей)"
                                },
                                "classroom_details": {
                                  "type": "object",
                                  "properties": {
                                    "address": {
                                      "type": "string"
                                    },
                                    "classroom_number": {
                                      "type": "string"
                                    },
                                    "computer_classroom": {
                                      "type": "boolean"
                                    },
                                    "online_classroom": {
                                      "type": "boolean"
                                    }
                                  },
                                  "description": "Аудитория (расширенный вариант)",
                                  "additionalProperties": false
                                }
                              }
                            }
                          },
                          "required": [
                            "time_start",
                            "time_end",
                            "text"
                          ],
                          "additionalProperties": false
                        }
                      }
                    },
                    "required": [
                      "day",
                      "month",
                      "week_day",
                      "lessons"
                    ],
                    "additionalProperties": false
                  }
                }
              }
            }
          },
          "4XX": {
            "description": "Default Response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "code": {
                      "type": "string"
                    },
                    "message": {
                      "type": "string"
                    },
                    "error": {
                      "type": "string"
                    },
                    "statusCode": {
                      "type": "number"
                    }
                  },
                  "required": [
                    "code",
                    "message"
                  ]
                }
              }
            }
          },
          "5XX": {
            "description": "Default Response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "code": {
                      "type": "string"
                    },
                    "message": {
                      "type": "string"
                    },
                    "error": {
                      "type": "string"
                    },
                    "statusCode": {
                      "type": "number"
                    }
                  },
                  "required": [
                    "code",
                    "message"
                  ]
                }
              }
            }
          }
        }
      }
    },
    "/calendar": {
      "get": {
        "tags": [
          "Расписание"
        ],
        "description": "Выдаёт расписание группы или преподавателя в iCal формате",
